<!-- views/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>List of Screens</title>
</head>
<body>
  <h1>List of Screens</h1>
  <ul>
    <% screens.forEach(screen => { %>
      <li>Screen ID: <%= screen.screen_id %>, Status: <%= screen.screenstatus %></li>
    <% }); %>
  </ul>
</body>
</html>
